{% extends 'base.html.twig' %}

{% block title %}Agent{% endblock %}

{% block body %}
    <h1>Agent</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ agent.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ agent.name }}</td>
            </tr>
            <tr>
                <th>Lat</th>
                <td>{{ agent.lat }}</td>
            </tr>
            <tr>
                <th>Lon</th>
                <td>{{ agent.lon }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ agent.city }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('agent_index') }}">back to list</a>

    <a href="{{ path('agent_edit', {'id': agent.id}) }}">edit</a>

    {{ include('agent/_delete_form.html.twig') }}
{% endblock %}
